<Window x:Class="Supermarketmanagement.PresentationLayer.Windows.EditProductWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Supermarketmanagement.PresentationLayer.Windows"
        mc:Ignorable="d"
        Title="EditProductWindow" Height="auto" Width="auto"
        HorizontalAlignment="Center"
             VerticalAlignment="Top"
        WindowStartupLocation="CenterScreen"
        MinHeight="700">
    <StackPanel HorizontalAlignment="Center" CanHorizontallyScroll="True">
        <Label>Sửa sản phẩm</Label>
        <TabControl Name="MainControl" Style="{StaticResource TabControl-SigleWindow}"></TabControl>
    </StackPanel>
</Window>
